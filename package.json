{
  "name": "absensi-app",
  "version": "1.0.0",
  "description": "Attendance system with backend and frontend",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd be-absensi && npm run start:dev",
    "dev:frontend": "cd fe-absensi2 && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd be-absensi && npm run build",
    "build:frontend": "cd fe-absensi2 && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd be-absensi && npm run start:prod",
    "start:frontend": "cd fe-absensi2 && npm run start",
    "start:pm2": "npm run build && pm2 start ecosystem.config.cjs",
    "stop:pm2": "pm2 stop ecosystem.config.cjs",
    "restart:pm2": "pm2 restart ecosystem.config.cjs",
    "delete:pm2": "pm2 delete ecosystem.config.cjs",
    "logs:pm2": "pm2 logs",
    "install:all": "npm install && cd be-absensi && npm install && cd ../fe-absensi2 && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "pm2": "^5.3.0"
  },
  "keywords": [
    "attendance",
    "backend",
    "frontend",
    "nestjs",
    "nextjs"
  ],
  "author": "",
  "license": "ISC"
}